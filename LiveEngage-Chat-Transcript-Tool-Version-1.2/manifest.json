{
    "name": "SyncExtension",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Storage Sync Extension",

    "permissions": ["storage", "tabs"],

    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {

    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
